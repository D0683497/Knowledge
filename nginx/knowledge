    charset utf-8;
    access_log /home/d0683497/Knowledge/nginx/nginx.access.log;
    error_log /home/d0683497/Knowledge/nginx/nginx.error.log;
    location /static {
        alias /home/d0683497/Knowledge/static/;
    }
    location /favicon.ico {
        root /home/d0683497/Knowledge/static/icon/favicon.ico;
    }
    location / {
        include uwsgi_params;
        uwsgi_pass django;
    }
}
